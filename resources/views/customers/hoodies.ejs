<section class="text-gray-600 body-font">
  <div class="container px-5 py-4 md:py-16 mx-auto">
    <div class="flex flex-wrap -m-4 text-center">
    <% hoodies.forEach((hoodies)=>{ %>
      <div class="lg:w-1/4 md:w-1/2 p-4 w-full shadow-lg">
        <a href="/product/?product=<%=hoodies.name%>" class="block relative rounded overflow-hidden cursor-pointer">
          <img
            alt="ecommerce"
            class="object-cover object-center w-full h-full block"
            src="/img/<%= hoodies.image %>"
          />
        </a>
        <div class="mt-4">
          <h3 class="text-gray-500 text-xs tracking-widest title-font mb-1">
            Hoodies
          </h3>
          <h2 class="text-gray-900 title-font text-lg font-medium">
            <%= hoodies.name %>
          </h2>
          <p class="mt-1">$â‚¹ <%= hoodies.price %></p>
          <p class="mt-1"><%= hoodies.size %></p>
        </div>
      </div>
      <%})%>
    </div>
  </div>
</section>
